{
  "name": "efflux-tracker",
  "version": "3.1.0",
  "type": "module",
  "description": "Application providing tracker based music creation within the web browser",
  "author": "Igor Zinken",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "server": "vite preview",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
