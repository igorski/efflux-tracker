    "@vitejs/plugin-vue": "^5.2.1",
    "audio-encoder": "1.0.4",
    "axios": "^1.7.2",
    "bowser": "^2.11.0",
    "eslint": "^8.36.0",
    "eslint-plugin-vue": "^8.7.1",
    "extendable-media-recorder": "^6.5.10",
    "extendable-media-recorder-wav-encoder": "^7.0.62",
